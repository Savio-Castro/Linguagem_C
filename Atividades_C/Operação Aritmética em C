#include <stdio.h>
#include <stdlib.h>

int main() {

  int A, B, X;
  A = 8;
  B = 4;
  
  X = A + B;
  printf("A soma é %d \n", X);
 
  X = A - B;
  printf("A subtração é %d \n", X);
  
  X = A * B;
  printf("A multiplicação é %d \n", X);
  
  X = A / B;
  printf("A Divisão é %d \n", X);

  return 0;
}
